{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-trailing-spaces": 0,
        "no-multiple-empty-lines": 0,
        "no-unsafe-optional-chaining": 0,
        "no-unused-vars": 0,
        "new-cap": 0,
        "import/prefer-default-export": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "lines-between-class-members": 0,
        "object-shorthand": 0
    }
}
